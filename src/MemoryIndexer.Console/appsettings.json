{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    }
  },
  "MemoryIndexer": {
    "Storage": {
      "Type": "InMemory",
      "ConnectionString": "memory.db",
      "CollectionName": "memories",
      "HnswM": 16,
      "HnswEfConstruction": 128,
      "HnswEfSearch": 64
    },
    "Embedding": {
      "Provider": "Ollama",
      "Model": "nomic-embed-text",
      "Dimensions": 768,
      "Endpoint": "http://localhost:11434",
      "BatchSize": 32,
      "CacheTtlMinutes": 60,
      "TimeoutSeconds": 30
    },
    "Scoring": {
      "RecencyWeight": 1.0,
      "ImportanceWeight": 1.0,
      "RelevanceWeight": 1.0,
      "DecayFactor": 0.99,
      "MaxExpectedAccessCount": 100
    },
    "Search": {
      "DefaultLimit": 5,
      "MaxLimit": 100,
      "MinScore": 0.0,
      "DenseWeight": 0.6,
      "SparseWeight": 0.4,
      "MmrLambda": 0.7,
      "DuplicateThreshold": 0.80,
      "RrfK": 60
    }
  }
}
